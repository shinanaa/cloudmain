@import "~common/stylus/variable"
#app
.moduleLayout
  height: calc(100% - 80px)
  .sidebar-container
    z-index: 10
    position:fixed
    top: 80px
    bottom: 0
    left: 0
    height: 100%
    .scrollbar-wrapper
      height: 100%
      .el-scrollbar__view
        height: 100%
        .el-menu-vertical-demo:not(.el-menu--collapse)
          width: $slide-width
        .el-menu
          height: 100%
          text-align:center
          .el-menu-item
            font-size: 17px
            font-weight: bold
